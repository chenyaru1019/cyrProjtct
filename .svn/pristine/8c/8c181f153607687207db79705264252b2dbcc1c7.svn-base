    <ion-content style="padding-top: 0px" ng-show="appType">
        <div style="width: 100%;text-align: center;padding-top: 20px;font-size: 18px;color: #6B6B6B" ng-if="listData.length == 0">
            暂无我的预定信息
        </div>
        <ion-list ng-repeat="item in listData | orderBy: 'supplyDt'" >
            <div class="row" style="padding-top: 5px;padding-bottom: 5px;padding-left: 10px;
                    padding-right: 10px;width: 100%;" ng-click="toPastyMyQuery1(item)">
                <div class="col-55" style="width: 55%">
                    {{item.supplyDt}}的点心
                </div>
                <div class="col-35" style="width: 35%">
                    {{item.states}}
                </div>
                <div class="col-10" style="text-align: right;width: 10%">
                    <img src="../img/btn_arrow.png" style="height: 15px">
                </div>
            </div>
            <div style="background-color: #DAC8C7;width: 100%;height: 1px">
            </div>
        </ion-list>

    </ion-content>

    <ion-content class="padding" style="padding-top: 0px" overflow-scroll="false" scroll="false" ng-show="!appType">
        <img src="../img/wrong.png" style="width: 33%;margin-left:33%;margin-top: 20%">
