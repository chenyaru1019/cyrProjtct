<ion-view view-title="" hide-nav-bar="true" style="background-color: #E5E5E4" hide-tabs="true" ng-show="appType">
    <ion-header-bar class="bar-clear" style="background-color: white;padding: 0px">
        <div style="width: 100%">
            <div class="row row-100" style="padding-bottom: 5px;padding-top: 5px;padding-left: 15px;padding-right: 15px;background-color: #E5E5E4">
                <div class="col-20"style="padding-top: 4px">
                    <!--<img src="../img/back.png" style="height: 20px">-->
                </div>
                <!--<div class="col-20" ng-click="volunteerMyQueryCancel()" style="padding-top: 4px">-->
                    <!--<img src="../img/back.png" style="height: 20px">-->
                <!--</div>-->
                <div  class="col-60" style="text-align: center;font-size: 20px;font-weight: bold;vertical-align: middle;padding-top: 5px;color: black">
                    服务详情
                </div>

                <!--<div class="qr-btn col-20" node-type="qr-btn2" style="text-align: right;overflow: hidden;position:relative;-->
                        <!--padding-top: 8px;" ng-click="sign()" ng-if="jsonData.bespeakState == '001'">-->
                    <!--<input node-type="jsbridge2" type="{{inputType}}" accept="image/*" style="position:absolute;left:0;top:0;z-index:2;-->
                    <!--opacity:0;"name="myPhoto" onchange="angular.element(this).scope().getImgFile(this.files)">-->
                    <!--<img src="../img/btn_checkin.png" style="height: 20px">-->
                <!--</div>-->
                <div class="col-20" style="text-align: right;padding-top: 8px;" ng-click="sign()" ng-if="jsonData.bespeakState == '001'">
                    <img src="../img/btn_checkin.png" style="height: 20px">
                </div>
            </div>
            <div style="background-color: #6B6B6B;height: 1px;width: 100%">
            </div>
        </div>
    </ion-header-bar>
    <ion-content style="padding-top: 0px" >
        <div class="row col-100" style="width: 100%;background-color: white">
            <div style="width: 15%;margin-left: -5px;margin-top: -5px">
                <img src="{{jsonData.signImg}}" style="height: 80px">
            </div>
            <div  style="width: 85%;padding-top: 5px;padding-bottom: 5px;padding-left:{{numL}}">
                <div style="font-size: 20px;font-weight: bold;padding-left: 3px">
                    {{jsonData.stationName}}
                </div>
                <div class="row row-100" style="padding-top: 10px;padding-bottom: 5px">
                    <label style="width: 75px">服务日期:</label> {{date}} {{jsonData.week}} {{jsonData.startH_m}}-{{jsonData.endH_m}}
                </div>
                <div class="row row-100" style="padding-bottom: 5px">
                    <label style="width: 70px">服务岗位:</label>{{jsonData.servicePost}}
                </div>
            </div>
        </div>
        <div ng-if="jsonData.bespeakState == '001'">
            <div class="col-100" style="background-color: white;margin-top: 10px;color: #68a948;padding-top: 8px;
                            padding-bottom: 8px;padding-left: 10px" ng-click="showConfirm()">
                取消预约
            </div>
            <!--<div style="background-color: white;height: 300px;width: 100%;margin-top: 10px">-->
                <!--<div style="color: #68a948;padding-left: 10px;padding-top: 10px">-->
                    <!--街区地图-->
                <!--</div>-->
            <!--</div>-->
            <div style="color: #6B6B6B;font-size: 16px;line-height: 100px;vertical-align: middle;padding-left: 10px" >
                如需取消预约，须提前三天取消
            </div>
        </div>

        <div ng-if="jsonData.bespeakState == '002'">
            <div style="padding: 10px">
                车站情况反馈
            </div>
            <div class="row" style="margin-bottom:10px;border-style: solid;border-color: #6B6B6B;border-radius: 10px;border-width: 1px;margin-left:10px;margin-right: 10px;width: auto">
                <div class="col-100">
                    {{feedback}}
                </div>
            </div>
            <div style="background-color: #6B6B6B;height: 1px;width: auto;margin-left: 10px;margin-right: 10px">
            </div>

            <div style="padding: 10px">
                星级评价
            </div>
            <div class="row row-100">
                <div class="col col-10" ng-repeat="item in stars" >
                    <img src="{{item.img}}" style="height: 30px" ng-click="starClick(item)">
                </div>
            </div>
            <div style="background-color: #6B6B6B;height: 1px;width: auto;margin-left: 10px;margin-right: 10px">
            </div>

            <div style="padding: 10px">
                照片反馈（限3张）
            </div>
            <div class="row row-100">
                <div class="col-20" style="height: 60px;width: 60px;padding-left: 20px" ng-if="ima1Show">
                    <img src="{{image1}}" style="height: 60px;width: 60px">
                </div>
                <div class="col-20" style="height: 60px;width: 60px" ng-if="ima2Show">
                    <img src="{{image2}}" style="height: 60px;width: 60px">
                </div>
                <div class="col-20" style="height: 60px;width: 60px" ng-if="ima3Show">
                    <img src="{{image3}}" style="height: 60px;width: 60px">
                </div>
            </div>

            <div style="background-color: #6B6B6B;height: 1px;width: auto;margin-left: 10px;margin-right: 10px">
            </div>

            <div style="padding: 10px">
                意见建议
            </div>
            <div style="border-width: 1px;border-style: solid;border-color: #6B6B6B;border-radius: 10px;
                height: 200px;padding-left: 10px;padding-right: 10px;margin-left: 10px;margin-right: 10px;margin-bottom: 10px">
                {{suggestion}}
            </div>

        </div>

        <div ng-if="jsonData.bespeakState == '003'" style="background-color: white;padding-top: 10px;padding-bottom: 10px">
            <div style="background-color: #6B6B6B;margin-left: 30px;margin-right: 30px;height: 1px;width: auto">
            </div>
            <div style="text-align: center;padding-top: 20px;width: 100%">
                <img src="../img/ic_tip_unfinshed.png" style="width: 100%;padding-left: 15px;padding-right: 15px">
            </div>
            <div style="font-size: 18px;color: #FC2F2F;text-align: center;padding-top: 5px">
                您本次任务未完成！！！！！！
            </div>
        </div>
        <div ng-if="jsonData.bespeakState == '004'" style="background-color: white;padding-top: 10px;padding-bottom: 10px">
            <div style="background-color: #6B6B6B;margin-left: 30px;margin-right: 30px;height: 1px;width: auto">
            </div>
            <div style="text-align: center;padding-top: 20px;width: 100%">
                <img src="../img/ic_tip_unattended.png" style="width: 100%;padding-left: 15px;padding-right: 15px">
            </div>
            <div style="font-size: 18px;color: #FC2F2F;text-align: center;padding-top: 5px">
                您本次任务已缺勤！！！！！！
            </div>
        </div>

    </ion-content>
</ion-view>


<ion-view view-title="" hide-nav-bar="true" style="background-color: white;z-index: 9999" hide-tabs="true" ng-show="!appType">
    <ion-content class="padding" style="padding-top: 0px" overflow-scroll="false" scroll="false">
        <img src="../img/wrong.png" style="width: 33%;margin-left:33%;margin-top: 20%">
        <div style="margin-top: 6%;text-align: center;width: 100%;height: 10%;font-size: 20px;color: #808080">当前浏览器无法打开地址</div>
    </ion-content>
</ion-view>



